    <command:examples>
      <command:example>
        <maml:title>-------------------------- EXAMPLE 1 --------------------------</maml:title>
        <dev:code>PS C:\&gt;Get-PrimaryDNSSuffix


Computername      : Bovine320
Domain            : globomantics.local
NV Domain         : globomantics.local
SynchronizeSuffix : True</dev:code>
        <dev:remarks>
          <maml:para>Get the DNS settings for the local computer.</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
